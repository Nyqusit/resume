        <img class="mb-4" src="/docs/4.2/assets/brand/bootstrap-solid.svg" alt="" width="72" height="72">
        <hr style="visibility: hidden">
        <h1 class="h3 mb-3 font-weight-normal"></h1>

      <div class="form-label-group">

        <%apartments.forEach((apartment,idx)=>{%>
          <div class="row mb-3" style="width:100%; ">
            <div class="col-md-8 themed-grid-col">
              <div class="pb-3">
                  <a href="http://nyquistable.com/apartment/<%=apartment._id%>"><%=apartment.owner%></a>
              </div>
              <div class="row">
                <div class="col-md-6 themed-grid-col"><%=apartment.address%></div>
                <div class="col-md-6 themed-grid-col">
                  <a href="http://nyquistable.com/users/myApartments/edit/<%=apartment._id%>">edit</a>
                  <a href="http://nyquistable.com/users/myApartments/delete/<%=apartment._id%>" style="color: red;">delete</a>
                  <!-- <script charset="utf-8"> -->
                  <!--     const send = document.getElementById("main-form"); -->
                  <!--     function deleteRequest(){ -->
                  <!--       //alert('delete ?'); -->
                  <!--       send.method = "DELETE"; -->
                  <!--       send.action = "http://10.0.0.43:3000/users/myApartments/delete/<%=apartment._id%>"; -->
                  <!--       send.submit(); -->
                  <!--     } -->
                      
                  <!-- </script> -->
                </div>
              </div>
            </div>
            <div class="col-md-4 themed-grid-col" style="margin:auto;">
              <img id="livinroomSlide" style="margin-bottom:-6px" src="http://nyquistable.com/users/<%=apartment.owner%>/img/<%=apartment.apartmentNumber%>/bedroom.jpg">
              <img id="diningroomSlide" style="margin-bottom:-6px" src="http://nyquistable.com/users/<%=apartment.owner%>/img/<%=apartment.apartmentNumber%>/diningroom.jpg">
              <img id="livingroomSlide" style="margin-bottom:-6px" src="http://nyquistable.com/users/<%=apartment.owner%>/img/<%=apartment.apartmentNumber%>/livingroom.jpg">
              <img id="livingroom2Slide" style="margin-bottom:-6px" src="http://nyquistable.com/users/<%=apartment.owner%>/img/<%=apartment.apartmentNumber%>/livingroom2.jpg">
            </div>
        </div>
        <br>
        <%});%>

      </div>
